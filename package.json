{
  "name": "react-leaflet-heatmap-layer",
  "version": "2.0.0",
  "description": "A custom layer for heatmaps in react-leaflet",
  "main": "lib/HeatmapLayer.js",
  "scripts": {
    "compile": "npm run clean; npm run transpile",
    "transpile": "./node_modules/.bin/babel src/HeatmapLayer.js --out-file lib/HeatmapLayer.js",
    "clean": "rm -rf ./lib/*",
    "example": "webpack --config ./example/webpack.config.js && webpack-dev-server --config ./example/webpack.config.js",
    "lint": "eslint ./src ./example/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenGov/react-leaflet-heatmap-layer.git"
  },
  "keywords": [
    "react",
    "leaflet",
    "layer",
    "map",
    "maps",
    "gis",
    "geo",
    "geojson",
    "heatmap",
    "heat"
  ],
  "author": "Jeremiah Hall <npm@jeremiahrhall.com>",
  "license": "MIT",
  "peerDependencies": {
    "leaflet": "^1.7.1",
    "react": "^17.0.2",
    "react-leaflet": "^2.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-transform-classes": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-stage-0": "^7.8.3",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-arrow-function": "^2.0.0",
    "eslint-plugin-lean-imports": "^0.3.3",
    "eslint-plugin-react": "^7.28.0",
    "leaflet": "^1.7.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-transform-hmr": "^1.0.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "dependencies": {
    "lodash.filter": "^4.6.0",
    "lodash.foreach": "^4.5.0",
    "lodash.isnumber": "^3.0.3",
    "lodash.map": "^4.6.0",
    "lodash.max": "^4.0.1",
    "lodash.min": "^4.0.1",
    "lodash.pluck": "^3.1.2",
    "lodash.reduce": "^4.6.0",
    "react-leaflet": "^2.8.0",
    "simpleheat": "^0.4.0"
  }
}
